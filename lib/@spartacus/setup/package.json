{
    "name": "@spartacus/setup",
    "version": "6.0.1",
    "description": "Includes features that makes Spartacus and it's setup easier and streamlined.",
    "keywords": [
        "spartacus",
        "framework",
        "storefront",
        "setup",
        "recipe"
    ],
    "homepage": "https://github.com/SAP/spartacus",
    "repository": "https://github.com/SAP/spartacus/tree/develop/core-libs/setup",
    "license": "Apache-2.0",
    "dependencies": {
        "tslib": "^2.4.0"
    },
    "peerDependencies": {
        "@angular/core": "^15.2.4",
        "@spartacus/cart": "6.0.1",
        "@spartacus/core": "6.0.1",
        "@spartacus/order": "6.0.1",
        "@spartacus/user": "6.0.1"
    },
    "optionalDependencies": {
        "@angular/platform-server": "^15.2.4",
        "@nguniversal/express-engine": "^15.2.0",
        "express": "^4.18.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "module": "fesm2015/spartacus-setup.mjs",
    "es2020": "fesm2020/spartacus-setup.mjs",
    "esm2020": "esm2020/spartacus-setup.mjs",
    "fesm2020": "fesm2020/spartacus-setup.mjs",
    "fesm2015": "fesm2015/spartacus-setup.mjs",
    "typings": "index.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./index.d.ts",
            "esm2020": "./esm2020/spartacus-setup.mjs",
            "es2020": "./fesm2020/spartacus-setup.mjs",
            "es2015": "./fesm2015/spartacus-setup.mjs",
            "node": "./fesm2015/spartacus-setup.mjs",
            "default": "./fesm2020/spartacus-setup.mjs"
        },
        "./ssr": {
            "types": "./ssr/index.d.ts",
            "esm2020": "./esm2020/ssr/spartacus-setup-ssr.mjs",
            "es2020": "./fesm2020/spartacus-setup-ssr.mjs",
            "es2015": "./fesm2015/spartacus-setup-ssr.mjs",
            "node": "./fesm2015/spartacus-setup-ssr.mjs",
            "default": "./fesm2020/spartacus-setup-ssr.mjs"
        }
    },
    "sideEffects": false
}